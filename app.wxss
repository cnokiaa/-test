/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
page{
	background: #eceff6;
}
.pageview {
	background: #eceff6;
}

/*弹窗提示*/
.popup,.pop-sign-in {
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.65);
	z-index: 1000;
}
.popup-con {
	position: absolute;
	left: 0;
	right: 0;
	top: 28.03%;
	overflow: hidden;
	width: 10rem;
	margin: 0 auto;
	background-color: #fff;
	-webkit-border-radius: 0.3rem;
	-moz-border-radius: 0.3rem;
	-ms-border-radius: 0.3rem;
	-o-border-radius: 0.3rem;
	border-radius: 0.3rem;
}
.popup-con p {
	padding: 1rem 1em;
	border-bottom: 1px solid #9c9f9f;
	color: #000;
	font: bold 0.6rem/0.93rem "Microsoft YaHei";
	text-align: center;
	word-break:break-all;
	display: block;
}
.popup-con div {
	display: block;
	color: #319aff;
	font: normal .7rem/2rem "Microsoft YaHei";
	text-align: center;
}

.loading {
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(0, 0, 0, 0.7);
	z-index: 900;
}
.loading div {
	position: absolute;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	width: 2rem;
	height: 2rem;
	margin: auto;
	background: url('../images/loading51.gif') no-repeat center center;
	-webkit-background-size: 100% 100%;
	-moz-background-size: 100% 100%;
	-ms-background-size: 100% 100%;
	-o-background-size: 100% 100%;
	background-size: 100% 100%;
}



/*导航*/
.nav {
	background-color: #fff;
}
.nav-step {
	padding: 0.6rem 7.81% 0;
}
.nav-inner {
	position: relative;
	width: 100%;
	padding-top: 9.26%;
	background: url('data:image/png;base64,') no-repeat 0 0;
	-webkit-background-size: 100% 100%;
	-moz-background-size: 100% 100%;
	-ms-background-size: 100% 100%;
	-o-background-size: 100% 100%;
	background-size: 100% 100%;
}
.nav-bar {
	height: 100%;
	background-color: #efeff4;
}
.progress {
	position: absolute;
	left: 4.63%;
	top: 50%;
	width: 90.74%;
	height: 0.15rem;
	margin-top: -0.05rem;
	background-color: #0084e9;
}
.nav-inner ul {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	display: -webkit-box;
	display: -moz-box;
	display: -ms-box;
	display: -o-box;
	display: box;
	-webkit-box-pack: justify;
	-moz-box-pack: justify;
	-ms-box-pack: justify;
	box-pack: justify;
}
.nav-inner ul li {
	display: block;
	position: relative;
	width: 9.26%;
	height: 100%;
	color: #090909;
	font-size: 0.6rem;
	line-height: normal;
	text-align: center;
}
.nav-inner li b {
	position: absolute;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	height: 1rem;
	margin: auto;
	line-height: 1rem;
	font-weight: normal;
}
.nav-inner ul .pass {
	background-color: #02a7e3;
	color: #fff;
	-webkit-box-shadow: none;
	-moz-box-shadow: none;
	-ms-box-shadow: none;
	-o-box-shadow: none;
	box-shadow: none;
}
.nav-tit{
	overflow: hidden;
	display: block;
}
.nav-tit li {
	display: block;
	float: left;
	width: 25%;
	height: 1.65rem;
	color: #000;
	font: normal 0.6rem/1.65rem "Microsoft YaHei";
	text-align: center;
}

.nav-inner ul li.ready {	
	background: url('../images/empty_dot.png') no-repeat center center;
	-webkit-background-size: 76%;
	-moz-background-size: 76%;
	-ms-background-size: 76%;
	-o-background-size: 76%;
	background-size: 76%;
}
.nav-inner ul li.active {
	background: url('../images/right.png') no-repeat center center;
	-webkit-background-size: 76%;
	-moz-background-size: 76%;
	-ms-background-size: 76%;
	-o-background-size: 76%;
	background-size: 76%;
}
.nav-tit li.active {
	color: #007aff;
}
.nav-inner ul li.active b,.nav-inner ul li.ready b {
	display: none;
}


.step1 li:first-child b {
	display: none;
}
.step1 .progress {
	width: 0;
}
.step2 .progress {
	width: 28%;
}
.step3 .progress {
	width: 58%;
}
.step4 .progress {
	width: 88%;
}