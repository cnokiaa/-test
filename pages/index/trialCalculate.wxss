header {
	position: relative;
	height: 2.2rem;
	border-bottom: 1px solid #c3c3c3;
	background-color: #fff;
	display: block;
}
header p {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 2.2rem;
	line-height: 2.2rem;
	text-align: center;
}
.application-record {
	display: block;
	position: absolute;
	left: 0.65rem;
	top: 0;
	padding-top: 0.2rem;
}
.application-record b {
	display: block;
	width: 1.15rem;
	height: 1.17rem;
	margin: 0 auto;
	background: url('data:image/png;base64,') no-repeat 0 0;
	-webkit-background-size: 100% 100%;
	-moz-background-size: 100% 100%;
	-ms-background-size: 100% 100%;
	-o-background-size: 100% 100%;
	background-size: 100% 100%;
}
.application-record span {
	display: block;
	height: 0.68rem;
	color: #15aee5;
	font-size: 0.7rem;
	line-height: 0.8rem;
	text-align: center;
}
.main-con {
	margin-bottom: 0.85rem;
	padding: 0.38rem 0.85rem 0;
	background-color: #fff;
	display:block;
}
.quota-tip {
	display: none;
	margin:0 0.2rem 0.1rem 0;
	color: #f00;
	font-size: 0.65rem;
	line-height: 1.15rem;
}
.quota-tip:before {
	float: left;
	width: 0.55rem;
	height: 100%;
	content: '\200B';
	background: url('../images/light.png') no-repeat center center;
	-webkit-background-size: 82%;
	-moz-background-size: 82%;
	-ms-background-size: 82%;
	-o-background-size: 82%;
	background-size: 82%;
}
.apply-tip {
	font-size: 0.75rem;
	line-height: 1.6rem;
	color: #3c3c3c;
}
.input-apply {
	display: -webkit-box;
	display: -moz-box;
	display: -ms-box;
	display: -o-box;
	display: box;
	height: 2.15rem;
	margin-bottom: 0.25rem;
	padding: 0 0.5rem;
	border: 2px solid #c5c5c5;
}
.input-apply label,
.input-apply input,
.input-apply span {
	display: block;
	font-size: 0.8rem;
}
.input-apply label,
.input-apply span {
	line-height: 2.15rem;
}
.input-apply input {
	border: none;
	height: 2.15rem;
	padding: 0 0.5rem;
	line-height: normal;
	outline: none;
	-webkit-box-flex: 1;
	-moz-box-flex: 1;
	-ms-box-flex: 1;
	box-flex: 1;
}
.input-apply span b {
	float: left;
	width: 0.8rem;
	height: 2.15rem;
	margin-right: 0.25rem;
}
.remove {
	background: url('../images/chacha.png') no-repeat center center;
	-webkit-background-size: 100%;
	-moz-background-size: 100%;
	-ms-background-size: 100%;
	-o-background-size: 100%;
	background-size: 100%;
}
.tips {
	font-size: 0.7rem;
	line-height: 1rem;
	padding-bottom: 0.6rem;
	border-bottom: 2px solid #c8c7cc;
	color:#b8b8b8;
	display: block;
}
/*申请周期*/
.apply-cycle {
	padding-top: 0.25rem;
	display: block;
}
.apply-cycle .h2 {
	font-size: 0.8rem;
	line-height: 1.85rem;
}
.cycle-part {
	overflow: hidden;
	display: block;
}
.cycle-part-con {
	width: 107%;
	display: block;
}
.cycle-part span {
	display: block;
	float: left;
	height: 1.85rem;
	padding:0 .5rem;
	margin: 0 1rem 0.7rem 0;
    border: 1px solid #03a7e4;
	font-size: 0.7rem;
	line-height: 1.85rem;
	text-align: center;
	border-radius: .3rem; 
}
.cycle-part .selected {
	color: #fff;
	background-color: #03a7e4;
}
/*还款明细*/
.replay-det {
	display: block;
	padding: 0.45rem 0.45rem;
	border:1px solid #0089f5;
	border-radius:.2rem;
}
.replay-det li {
	display: block;
	height: 1.2rem;
	overflow: hidden;
	zoom:1;
}
.replay-det li span,
.replay-det li strong {
	color: #3c3c3c;
	height: 100%;
	font-size: 0.73rem;
	line-height: 1rem;
}
.replay-det strong em {
	color: #f00;
	font-weight: normal;
}
.replay-det li span {
	float: left;
}
.replay-det li strong {
	float: right;
}
.replay-det li a {
	float: right;
	height: 100%;
	color: #4d97e1;
	font-size: 0.53rem;
	line-height: 0.82rem;
}
.next-step {
	display: block;
	width: 93.75%;
	height: 1.9rem;
	margin: 0 auto 1rem;
	background-color: #0d7aff;
	color: #fff;
	font-size: 0.8rem;
	line-height: 1.9rem;
	text-align: center;
	-webkit-border-radius: 0.13rem;
	-moz-border-radius: 0.13rem;
	-ms-border-radius: 0.13rem;
	-o-border-radius: 0.13rem;
	border-radius: 0.13rem;
}
.disabled {
	background-color: #b6b6ba;
}

.red{color: #f00;}
.connect {
	position: absolute;
	right: 0.5rem;
	top: 0;
	width: 1.25rem;
	height: 100%;
}
.connect strong {
	display: block;
	width: 100%;
	height: 100%;
	background: url('data:image/png;base64,') no-repeat center center;
	-webkit-background-size: 100%;
	-moz-background-size: 100%;
	-ms-background-size: 100%;
	-o-background-size: 100%;
	background-size: 100%;
}
.connect-con {
	position: absolute;
	top:2.2rem;
	right: -0.5rem;
	width: 4rem;
	background-color: #eceff6;
}
.connect-con a,
.connect-con i {
	display: block;
	height: 1.5rem;
	color: #3c3c3c;
	font-size: 0.7rem;
	font-style: normal;
	text-align: center;
	line-height: 1.5rem;
}
.connect-con a {
	border-bottom: 1px solid #c3c3c3;
}

.usemoney{
	display: block;
    overflow: hidden;
    margin: 1rem -0.85rem;
    font-size:.8rem;
    border-top: 1rem solid #efeff4;
    padding: 0 0.85rem;
    line-height:2.5rem;
	
}
.usemoney span{
	float:left;
}
.usemoney strong{
	float:right;
}
.usemoney strong{display: block;}
.usemoney strong a:after{
	content:'';
	width:.5rem;
    height:.5rem;
    border-right:2px solid #c4c4c4;
    border-bottom:2px solid #c4c4c4;
    -moz-transform:rotate(-45deg);
    -o-transform:rotate(-45deg);
    -webkit-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    transform: rotate(-45deg);
    display: inline-block;
    margin: 0 .2rem;
}



/*登陆*/
.sign-in-pan {
	position: absolute;
	left: 0;
	right: 0;
	top: 18.03%;
	overflow: hidden;
	width: 13rem;
	margin: 0 auto;
	padding-top: 1.23rem;
	background-color: #fff;
	-webkit-border-radius: 0.38rem;
	-moz-border-radius: 0.38rem;
	-ms-border-radius: 0.38rem;
	-o-border-radius: 0.38rem;
	border-radius: 0.38rem;
}
.sign-in-pan h3 {
	margin-bottom: 1.52rem;
	color: #000;
	font-size: 0.7rem;
	line-height: 1.2rem;
	text-align: center;
}
.sign-in-mobile,
.sign-in-res {
	height: 1.25rem;
	display: -webkit-box;
	display: -moz-box;
	display: -ms-box;
	display: -o-box;
	display: box;
	-webkit-box-pack: center;
	-moz-box-pack: center;
	-ms-box-pack: center;
	box-pack: center;
}
.sign-in-mobile {
	margin-bottom: 0.78rem;
}
.sign-in-res {
	margin-bottom: 2.45rem;
}
.sign-in-con label,
.sign-in-con input {
	display: block;
	color: #3c3c3c;
	font-size: 0.6rem;
}
.sign-in-con label {
	line-height: 1.25rem;
}
.sign-in-mobile input {
	width: 7.5rem;
}
.sign-in-res input {
	width: 3.75rem;
	margin-right: 0.23rem;
}
.sign-in-mobile input,
.sign-in-res input {
	height: 100%;
	border: 1px solid #abb2bc;
	line-height: normal;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-ms-box-sizing: border-box;
	-o-box-sizing: border-box;
	box-sizing: border-box;
	-webkit-border-radius: 0.13rem;
	-moz-border-radius: 0.13rem;
	-ms-border-radius: 0.13rem;
	-o-border-radius: 0.13rem;
	border-radius: 0.13rem;
}
.sign-in-res div {
	overflow: hidden;
	width: 3.5rem;
	height: 100%;
	text-align: center;
	line-height: 1.25rem;
	-webkit-border-radius: 0.13rem;
	-moz-border-radius: 0.13rem;
	-ms-border-radius: 0.13rem;
	-o-border-radius: 0.13rem;
	border-radius: 0.13rem;
}
.send {
	display: block;
	width: 100%;
	height: 100%;
	background-color: #007aff;
	color: #fff;
	font-size: 0.5rem;
}
.settime {
	display: none;
	width: 100%;
	height: 100%;
	background-color: #c4c4c4;
	color: #fff;
	font-size: 0.5rem;
}
.sign-btn {
	border-top: 1px solid #b2b2b2;
}
.sign-btn b,
.sign-btn strong {
	float: left;
	width: 50%;
	font-size: 0.6rem;
	font-weight: normal;
	line-height: 1.75rem;
	text-align: center;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-ms-box-sizing: border-box;
	-o-box-sizing: border-box;
	box-sizing: border-box;
}
.sign-btn b {
	color: #3c3c3c;
	border-right: 1px solid #b2b2b2;
}
.sign-btn strong {
	color: #007aff;
}
.sign-btn p {
	display: none;
}
.logining b,
.logining strong {
	display: none;
}
.logining p {
	display: block;
	color: #3c3c3c;
	font-size: 0.6rem;
	font-weight: normal;
	line-height: 1.75rem;
	text-align: center;
}
.fm{font-style: inherit;}